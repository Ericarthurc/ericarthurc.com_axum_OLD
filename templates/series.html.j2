{% extends "./layouts/head.html.j2" %}

{% block title %}{% endblock %}

{% block head %}{% endblock %}

{% block content %}
  {% include "includes/header_logo.html.j2" %}
  {% include "./includes/navbar.html.j2" %}
  <div class="main-container">
    <p class="main-header">{{series}} Series</p>
    <div class="post-list">
      {% for series in series_meta %}
      <div class="post-container">
        <a class="post-anchor" href="/blog/{{series.file_name}}">
          <div class="post-info">
            <span class="post-info-title"> {{series.title}} </span>
            <span class="post-info-date"> {{series.date}} </span>
          </div>
        </a>
      </div>
      {% endfor %}
    </div>
  </div>
{% endblock %}