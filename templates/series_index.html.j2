{% extends "./layouts/head.html.j2" %}

{% block title %}{% endblock %}

{% block head %}{% endblock %}

{% block content %}
  {% include "includes/header_logo.html.j2" %}
  {% include "./includes/navbar.html.j2" %}
  <div class="main-container">
    <div class="index-list">
      {% for series in series_index %}
      <div class="main_blog-post">
        <a class="list-anchor" href="/series/{{series}}">
          <div class="list-anchor__info">
            <span class="info-title"> {{series}} </span>
          </div>
        </a>
      </div>
      {% endfor %}
    </div>
  </div>
{% endblock %}